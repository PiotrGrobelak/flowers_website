{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
        "document": true,
        "window": true,
        "process": true,
        "require": true,
        "module": true,
        "__dirname": true,
        "console": true
    },
  "rules": {
    "strict": 0,
    "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "react/prop-types": [1],
    "no-console": "off",
    "react/display-name": [0]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}